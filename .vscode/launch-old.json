{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "args": ["${relativeFile}"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
